{
  "name": "jira-timer",
  "version": "1.0.24",
  "description": "Jira Timer",
  "productName": "Jira Timer",
  "main": "index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'node -r babel-register server.js' 'npm start'",
    "pack": "NODE_ENV=production webpack --config webpack.config.production.babel.js",
    "build": "npm run pack && electron-builder",
    "build:linux": "npm run pack && electron-builder --linux",
    "release": "npm --no-git-tag-version version patch && npm run pack && electron-builder --linux --mac -p always && opn https://github.com/alexcroox/jira-timer-menubar/releases && npm run postinstall"
  },
  "author": "Alex Crooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcroox/jira-timer"
  },
  "license": "MIT",
  "engine-strict": true,
  "build": {
    "productName": "Jira Timer",
    "appId": "com.somethinginteresting.jira-timer",
    "publish": [
      {
        "provider": "github",
        "owner": "alexcroox",
        "repo": "jira-timer-menubar"
      }
    ],
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "static/",
      "index.html",
      "index.js",
      "jira-worklogs.js",
      "auto-updater.js",
      "node_modules/",
      "package.json",
      "!**/signing/"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/app.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "static",
      "category": "Utility"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "array-flatten": "^2.1.2",
    "auto-launch": "^5.0.5",
    "contra": "^1.9.4",
    "date-fns": "^1.29.0",
    "delay": "^4.1.0",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
    "electron-updater": "4.0.5",
    "fix-path": "^2.1.0",
    "hh-mm-ss": "^1.2.0",
    "history": "^4.7.2",
    "keytar": "^4.3.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isempty": "^4.4.0",
    "lodash.orderby": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "menubar": "github:mantou132/menubar#master",
    "node-loader": "^0.6.0",
    "object-get": "^2.1.0",
    "opn": "^5.4.0",
    "parse-duration": "^0.1.1",
    "performance-now": "^2.1.0",
    "pretty-ms": "^4.0.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-seamless-immutable": "^0.4.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "revalidation": "^0.12.1",
    "seamless-immutable": "^7.1.4",
    "string-template": "^1.0.0",
    "styled-components": "^4.1.2",
    "styled-components-spacing": "^3.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.26.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "devtron": "^1.4.0",
    "electron": "^3.0.13",
    "electron-builder": "^20.38.2",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "json": "^9.0.6",
    "npm-version": "^1.1.0",
    "opn-cli": "^4.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  }
}
